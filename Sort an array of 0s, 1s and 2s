 #include <bits/stdc++.h>
 using namespace std;
 void printarr(int arr[], int n)
 {
	 for(int i=0;i<n;i++)
	 {
		 cout<<arr[i]<<" ";
	 }
 }
 void aslicheez(int arr[], int n)
 {
	 int cnt0=0,cnt1=0,cnt2=0;
	 for(int i=0;i<n;i++)
	 {
		 if(arr[i]==0)
		 	cnt0++;
			 
		if(arr[i]==1)
			cnt1++;
			
			if(arr[i]==2)
			cnt2++;
				 
	 }

	 int i=0;
	 while(cnt0>0)
	 {
	 	cout<<arr[i++]<<" ";
		 cnt0--;
	 }
	while(cnt1>0)
	{
		cout<<arr[i++]<<" ";
		cnt1--;
	}
		while(cnt2>0)
		{
		cout<<arr[i++]<<" ";
		cnt2--;
		}	 
 }
 
 
 
 int main()
 {
	 int arr[]={ 0, 1, 1, 0, 1, 2, 1, 2, 0, 0, 0, 1 };
	 int n=sizeof(arr)/sizeof(arr[0]);
	printarr(arr,n);
	aslicheez(arr,n);
	printarr(arr,n);





 }
