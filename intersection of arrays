// program to fnind the intersection of the sorted arraya 

#include <bits/stdc++.h>
using namespace std ;

void jugaadfunction(int arr1[], int arr2[], int n1, int n2)
{
	int i=0,j=0;
	while(i<n1 && j<n2)
	{
		if(arr1[i]==arr2[j])
		{
			cout<<arr1[i++]<<" ";
			j++;

		}
		if(arr1[i]>arr2[j])
		{
			j++;
		}
		if(arr1[i]<arr2[j])
		{
			i++;
		}
	}

}





int main()
{
	int arr1[]={1,3,4,5};
	int arr2[]={1,2,3,4,5};
	int n1=sizeof(arr1)/sizeof(arr1[0]);

	int n2=sizeof(arr2)/sizeof(arr2[0]);

	jugaadfunction(arr1,arr2,n1,n2);



}
