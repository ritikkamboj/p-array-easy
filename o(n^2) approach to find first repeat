// program to find the first reapeated elemetn in the array


#include <bits/stdc++.h>
using namespace std;
int main()
{
	int j;
	int arr[]={6, 10, 5, 4, 9, 120, 4, 6, 10};
	int n=sizeof(arr)/sizeof(arr[0]);
	for(int i=0;i<n-1;i++)
	{
		for(j=i+1;j<n;j++)
		{
			if(arr[i]==arr[j])
			{
			cout<<arr[j];
			break;
			}
			

		}
		if(arr[i]==arr[j])
		break;
		//break;
	}




}
